  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test --env=jsdom --verbose --silent",
    "coverage": "react-scripts test --coverage --watchAll --collectCoverageFrom='src/{components,pages}/**/*.{ts,tsx}' --collectCoverageFrom='src/shared/functions/*.{ts,tsx}' --collectCoverageFrom='!src/pages/**/{index,RightTag,LeftTag,PaginationTag}.{ts,tsx}' --env=jsdom --verbose --silent",
    "eject": "react-scripts eject",
    "predeploy": "react-app-rewired build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
