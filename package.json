    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.1.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "transform": {
      "^.+\\.css$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "react-datepicker/dist/react-datepicker.css": "jest-transform-stub",
      "react-step-progress-bar/styles.css": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|my-ecmascript-lib)/)"
    ]
  }
